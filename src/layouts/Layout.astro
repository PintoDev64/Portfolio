---
import Navbar from '../components/Navbar.astro';

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
	<head>
		<meta charset="utf-8"/>
        <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico"/>
        <meta name="theme-color" content="#000000"/>
        <link rel="apple-touch-icon" href="/logo512.png"/>
        <link rel="manifest" href="/manifest.json"/>
        <link rel="canonical" href="https://www.noobapro.xyz"/>
        <meta name="description" content="Amante de la tecnologia web"/>
        <meta name="viewport" content="width=device-width"/>
        <meta property="og:title" content="Joan Cardozo | FullStack Web Developer"/>
        <meta property="og:image" content="/joancardozo.png"/>
        <meta property="og:image:width" content="1280"/>
        <meta property="og:image:height" content="720"/>
        <meta property="og:url" content="https://www.noobapro.xyz"/>
        <meta property="og:site_name" content="Joan Cardozo | FullStack Web Developer"/>
        <meta property="og:type" content="website">
        <meta property="og:description" content="Sistema para enviar sus demos/partidas directamente al canal Znorux en Youtube"/>
        <meta name="twitter:card" content="summary_large_image">
        <meta name="twitter:title" content="Joan Cardozo | FullStack Web Developer"/>
        <meta name="twitter:description" content="Amante de la tecnologia web"/>
        <meta name="twitter:image" content="/joancardozo.png"/>
        <meta name="twitter:image:alt" content="Joan Cardozo | FullStack Web Developer"/>
        <title>{ title }</title>
	</head>
	<body>
        <Navbar />
        <main>
            <slot />
        </main>
	</body>
</html>
<style is:global>
	:root {
		
	}
</style>
